#!/usr/bin/env bash
# comments here
export a=1
while [[ a -le 100 ]]
do
      if (( a % 3 == 0 ))  && (( a % 5 == 0 ));
then echo "FizzBuzz"
elif (( a % 3 == 0 ));
then echo "Fizz"
elif (( a % 5 == 0 ));
then echo "Buzz"
else
    echo $a
  
fi
      (( a += 1 ))
      done
     shellcheck 10-fizzbuzz
